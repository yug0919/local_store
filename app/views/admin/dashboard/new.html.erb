<h2>Admin Login</h2>

<%= form_with url: admin_login_path, method: :post do |f| %>
  <p>
    <%= f.label :email %><br>
    <%= f.email_field :email %>
  </p>

  <p>
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </p>

  <p>
    <%= f.submit "Login" %>
  </p>
<% end %>

<%= flash[:alert] if flash[:alert] %>
